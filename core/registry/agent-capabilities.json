    "insight_agent": {
      "id": "agent-insight",
      "name": "Insight Analysis Agent",
      "description": "Specialized in code analysis, pattern recognition, and architectural insights",
      "capabilities": {
        "code_analysis": {
          "level": "expert",
          "features": [
            "static_analysis",
            "complexity_metrics",
            "dependency_mapping",
            "code_smell_detection",
            "performance_profiling"
          ],
          "languages": ["python", "javascript", "typescript", "rust", "go"],
          "metrics": {
            "cyclomatic_complexity": true,
            "code_coverage": true,
            "duplication_detection": true,
            "security_vulnerabilities": true
          }
        },
        "pattern_recognition": {
          "level": "expert",
          "patterns": [
            "design_patterns",
            "anti_patterns",
            "architectural_patterns",
            "refactoring_opportunities"
          ],
          "learning": {
            "continuous": true,
            "feedback_integration": true,
            "pattern_evolution": true
          }
        },
        "architectural_analysis": {
          "level": "advanced",
          "capabilities": [
            "layer_identification",
            "component_coupling",
            "cohesion_analysis",
            "scalability_assessment"
          ]
        }
      },
      "limitations": [
        "Cannot execute code directly",
        "Limited to static analysis",
        "Requires structured input"
      ],
      "integrations": ["orchestrator", "execution_agent", "testing_agent"]
    },
    "execution_agent": {
      "id": "agent-execution",
